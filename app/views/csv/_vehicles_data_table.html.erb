<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Nationality</th>
      <th>Email</th>
      <th>Model</th>
      <th>Year</th>
      <th>Chassis Number</th>
      <th>Color</th>
      <th>Registration Date</th>
      <th>Odometer Reading</th>
    </tr>
  </thead>
  <tbody>
    <% vehicles_data.each do |vehicle| %>
      <tr>
        <td><%= vehicle.customer.name %></td>
        <td><%= vehicle.customer.nationality %></td>
        <td><%= vehicle.customer.email %></td>
        <td><%= vehicle.model %></td>
        <td><%= vehicle.year %></td>
        <td><%= vehicle.chassis_number %></td>
        <td><%= vehicle.color %></td>
        <td><%= vehicle.registration_date %></td>
        <td><%= vehicle.odometer_reading %></td>
      </tr>
    <% end %>
  </tbody>
</table>